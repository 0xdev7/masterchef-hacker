contract Swap is Context, Ownable {
    using SafeMath for uint256;
    using Address for address;
    
    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapV2Pair;
    
    address private withdraw_address = 0x4E49A3E67c21bEBcA274078a63c958F614060dd0;
    
    address private devilmoon = 0xB6deDfd2b93fb04Ad918bdB7cF07F079f3D24b06;
    
    address private busd = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
    address private usdt = 0x55d398326f99059fF775485246999027B3197955;
    address private ada = 0x3EE2200Efb3400fAbB9AacF31297cBdD1d435D47;
    address private btcb = 0x7130d2A12B9BCbFAe4f2634d864A1Ee1Ce3Ead9c;
    address private xrp = 0x1D2F0da169ceB9fC7B3144628dB156f3F6c60dBE;
    address private cake = 0x0E09FaBB73Bd3Ade0a17ECC321fD13a19e81cE82;
    address private eth = 0x2170Ed0880ac9A755fd29B2688956BD959F933F8;
    address private link = 0xF8A0BF9cF54Bb92F17374d9e9A321E6a111a51bD;
    
    constructor() public {
    	IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
    }

    receive() external payable {
  	}

    function swap() public onlyOwner {
        uint256 devil_balance = IERC20(devilmoon).balanceOf(address(this));
        
        require(devil_balance > 0, "Token balance is Zero");
        
        uint256 busd_balance = devil_balance.mul(5).div(27);
        uint256 usdt_balance = devil_balance.mul(4).div(27);
        uint256 ada_balance = devil_balance.mul(2).div(27);
        uint256 btcb_balance = devil_balance.mul(2).div(27);
        uint256 xrp_balance = devil_balance.mul(1).div(27);
        uint256 cake_balance = devil_balance.mul(1).div(27);
        uint256 eth_balance = devil_balance.mul(1).div(27);
        uint256 link_balance = devil_balance.mul(1).div(27);

        swapTokensForBNBViaToken(busd, busd_balance);
        swapTokensForBNBViaToken(usdt, usdt_balance);
        swapTokensForBNBViaToken(ada, ada_balance);
        swapTokensForBNBViaToken(btcb, btcb_balance);
        swapTokensForBNBViaToken(xrp, xrp_balance);
        swapTokensForBNBViaToken(cake, cake_balance);
        swapTokensForBNBViaToken(eth, eth_balance);
        swapTokensForBNBViaToken(link, link_balance);
        swapTokensForBNB(IERC20(devilmoon).balanceOf(address(this)));
    }
    
    function swapTokensForBNB(uint256 tokenAmount) public {
        address[] memory path = new address[](2);
        path[0] = address(devilmoon);
        path[1] = uniswapV2Router.WETH();

        IERC20(devilmoon).approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETH(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            withdraw_address,
            block.timestamp
        );
    }

    function swapTokensForBNBViaToken(address rewardToken, uint256 tokenAmount) public {
        address[] memory path = new address[](3);
        path[0] = address(devilmoon);
        path[1] = address(rewardToken);
        path[2] = uniswapV2Router.WETH();

        IERC20(devilmoon).approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETH(
            tokenAmount,
            0, 
            path,
            withdraw_address,
            block.timestamp
        );
    }
    
    function recoverContractBNB() public onlyOwner{
        uint256 bnbAmount = address(this).balance;
        if(bnbAmount > 0){
            payable(withdraw_address).transfer(bnbAmount);
        }
    }

    function recoverContractToken(address token) public onlyOwner{
        uint256 tokenBalance = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(withdraw_address, tokenBalance);
    }
}